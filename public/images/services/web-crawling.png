<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f172a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="webGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bgGrad)"/>
  
  <!-- Web spider/crawler icon -->
  <g transform="translate(200,150)">
    <!-- Spider body -->
    <ellipse cx="0" cy="0" rx="25" ry="18" fill="url(#webGrad)"/>
    
    <!-- Spider legs -->
    <g stroke="url(#webGrad)" stroke-width="3" fill="none">
      <!-- Left legs -->
      <path d="M-25,-10 L-45,-25 L-50,-35"/>
      <path d="M-25,-5 L-50,-5 L-55,-5"/>
      <path d="M-25,5 L-50,5 L-55,5"/>
      <path d="M-25,10 L-45,25 L-50,35"/>
      
      <!-- Right legs -->
      <path d="M25,-10 L45,-25 L50,-35"/>
      <path d="M25,-5 L50,-5 L55,-5"/>
      <path d="M25,5 L50,5 L55,5"/>
      <path d="M25,10 L45,25 L50,35"/>
    </g>
    
    <!-- Spider eyes -->
    <circle cx="-8" cy="-8" r="3" fill="#ffffff"/>
    <circle cx="8" cy="-8" r="3" fill="#ffffff"/>
    <circle cx="-8" cy="-8" r="1.5" fill="#000000"/>
    <circle cx="8" cy="-8" r="1.5" fill="#000000"/>
  </g>
  
  <!-- Web network -->
  <g stroke="#10b981" stroke-width="2" fill="none" opacity="0.6">
    <!-- Horizontal lines -->
    <line x1="50" y1="80" x2="350" y2="80"/>
    <line x1="50" y1="120" x2="350" y2="120"/>
    <line x1="50" y1="180" x2="350" y2="180"/>
    <line x1="50" y1="220" x2="350" y2="220"/>
    
    <!-- Vertical lines -->
    <line x1="80" y1="50" x2="80" y2="250"/>
    <line x1="150" y1="50" x2="150" y2="250"/>
    <line x1="250" y1="50" x2="250" y2="250"/>
    <line x1="320" y1="50" x2="320" y2="250"/>
  </g>
  
  <!-- Data nodes -->
  <g fill="#10b981">
    <circle cx="80" cy="80" r="4"/>
    <circle cx="150" cy="120" r="4"/>
    <circle cx="250" cy="180" r="4"/>
    <circle cx="320" cy="220" r="4"/>
    <circle cx="150" cy="180" r="4"/>
    <circle cx="250" cy="120" r="4"/>
  </g>
  
  <!-- Title -->
  <text x="200" y="40" text-anchor="middle" fill="#10b981" font-family="Arial, sans-serif" font-size="24" font-weight="bold">
    웹 크롤링 자동화
  </text>
  
  <!-- Subtitle -->
  <text x="200" y="280" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="14">
    지능형 데이터 수집 • 실시간 모니터링
  </text>
</svg>