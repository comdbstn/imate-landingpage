<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f172a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="landingGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2563eb;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="pageGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bgGrad)"/>
  
  <!-- Browser window -->
  <g transform="translate(50,60)">
    <!-- Browser frame -->
    <rect x="0" y="0" width="300" height="200" rx="8" ry="8" fill="#334155" stroke="#64748b" stroke-width="2"/>
    
    <!-- Browser top bar -->
    <rect x="0" y="0" width="300" height="25" rx="8" ry="8" fill="#475569"/>
    <rect x="0" y="12.5" width="300" height="12.5" fill="#475569"/>
    
    <!-- Browser buttons -->
    <circle cx="15" cy="12.5" r="4" fill="#ef4444"/>
    <circle cx="30" cy="12.5" r="4" fill="#f59e0b"/>
    <circle cx="45" cy="12.5" r="4" fill="#10b981"/>
    
    <!-- Address bar -->
    <rect x="70" y="6" width="220" height="13" rx="6" ry="6" fill="#64748b"/>
    <text x="85" y="15" fill="#94a3b8" font-family="Arial, sans-serif" font-size="10">yoursite.com</text>
    
    <!-- Page content -->
    <rect x="8" y="33" width="284" height="159" rx="4" ry="4" fill="url(#pageGrad)"/>
    
    <!-- Hero section -->
    <rect x="15" y="40" width="270" height="60" fill="url(#landingGrad)" opacity="0.3"/>
    <rect x="25" y="50" width="120" height="8" fill="#3b82f6"/>
    <rect x="25" y="62" width="180" height="4" fill="#64748b" opacity="0.7"/>
    <rect x="25" y="68" width="160" height="4" fill="#64748b" opacity="0.7"/>
    <rect x="25" y="78" width="80" height="16" rx="8" ry="8" fill="#3b82f6"/>
    
    <!-- Features section -->
    <g transform="translate(25,110)">
      <!-- Feature boxes -->
      <rect x="0" y="0" width="60" height="40" fill="#f1f5f9" stroke="#e2e8f0"/>
      <rect x="5" y="5" width="50" height="6" fill="#3b82f6" opacity="0.3"/>
      <rect x="5" y="15" width="40" height="3" fill="#64748b" opacity="0.5"/>
      <rect x="5" y="20" width="45" height="3" fill="#64748b" opacity="0.5"/>
      <circle cx="50" cy="30" r="5" fill="#10b981"/>
      
      <rect x="70" y="0" width="60" height="40" fill="#f1f5f9" stroke="#e2e8f0"/>
      <rect x="75" y="5" width="50" height="6" fill="#3b82f6" opacity="0.3"/>
      <rect x="75" y="15" width="40" height="3" fill="#64748b" opacity="0.5"/>
      <rect x="75" y="20" width="45" height="3" fill="#64748b" opacity="0.5"/>
      <circle cx="120" cy="30" r="5" fill="#10b981"/>
      
      <rect x="140" y="0" width="60" height="40" fill="#f1f5f9" stroke="#e2e8f0"/>
      <rect x="145" y="5" width="50" height="6" fill="#3b82f6" opacity="0.3"/>
      <rect x="145" y="15" width="40" height="3" fill="#64748b" opacity="0.5"/>
      <rect x="145" y="20" width="45" height="3" fill="#64748b" opacity="0.5"/>
      <circle cx="190" cy="30" r="5" fill="#10b981"/>
    </g>
    
    <!-- CTA section -->
    <rect x="80" y="165" width="140" height="20" rx="10" ry="10" fill="#3b82f6"/>
    <text x="150" y="177" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Get Started</text>
  </g>
  
  <!-- Conversion arrows -->
  <g stroke="#10b981" stroke-width="3" fill="none" marker-end="url(#arrowhead)">
    <defs>
      <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="10" refY="3.5" orient="auto">
        <polygon points="0 0, 10 3.5, 0 7" fill="#10b981"/>
      </marker>
    </defs>
    <path d="M30,180 C20,200 30,220 50,230" stroke-dasharray="3,3"/>
    <path d="M370,180 C380,200 370,220 350,230" stroke-dasharray="3,3"/>
  </g>
  
  <!-- Conversion indicators -->
  <text x="40" y="245" fill="#10b981" font-family="Arial, sans-serif" font-size="12" font-weight="bold">고전환율</text>
  <text x="320" y="245" fill="#10b981" font-family="Arial, sans-serif" font-size="12" font-weight="bold">SEO최적화</text>
  
  <!-- Title -->
  <text x="200" y="40" text-anchor="middle" fill="#3b82f6" font-family="Arial, sans-serif" font-size="24" font-weight="bold">
    랜딩페이지 제작
  </text>
  
  <!-- Subtitle -->
  <text x="200" y="280" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="14">
    고전환율 • SEO 최적화 • 반응형 디자인
  </text>
</svg>